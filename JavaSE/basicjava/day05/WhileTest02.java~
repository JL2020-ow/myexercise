/*
  使用while循环输出1~10
  
*/
public class WhileTest02
{
   public static void main(String[] args)
   {
      int i;
      while(i<=10)
      {
        System.out.println(i);
        i++;
      }
        System.out.println("end-->"+i);
        System.out.println("-----------------------------");
        int j=10;
       while(j>0){
           
       }
      System.out.println("end--->"+j); 
   }
}